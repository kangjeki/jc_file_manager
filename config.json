{	
	"__comment"		: "ganti primary path pada setting untuk akses directory, contoh C:/xampp/htdocs",

	"app_name" 		: "JC File Manager",
	"version"		: "v1.0",
	"description" 	: "Copyright: 2019 | JC Boster Project",
	"author"		: "JC_Programs",

	"setting" 		: 
	{
		"path" 					: "C:/xampp/htdocs",
		"content_path"			: "default",
		"protocol" 				: "http://",			
		"host" 					: "localhost",
		"default_file_read"		: "txt",
		"default_mime_type" 	: "text",
		"text_highlight"		: true
	},
	"plugin" 		: {
		"text_highlight" : "JC_DefaultTextHighLight"
	},

	"mimeType" 		: 
	{
		"type" : 
		{
			"0"	: "text",
			"1" : "audio",
			"2" : "video",
			"3" : "image",
			"4" : "folder",
			"5" : "arcive"
		},
		"extensions" :
		[
			["php", "html", "css", "txt", "js", "jcm", "jc", "json", "md", "xls", "sql", "doc"],
			["wav", "mp3", "oog"],
			["mp4", "avi", "3gp", "webm"],
			["jpg", "jpeg", "png"],
			["folder"],
			["rar", "zip", "apk", "gzip"]
		]
	}
}
